{
    "name": "frontend-mentor-library",
    "version": "1.0.0",
    "description": "",
    "scripts": {
        "start": "serve dist",
        "sync:assets": "rsync -av --delete --exclude=.DS_Store --exclude=*.scss --exclude=*.js --exclude=*.html ../../assets/media/dist/ ./assets/",
        "develop": "export NODE_ENV=development && webpack --config build/webpack.dev.js --watch --progress --stats-children",
        "build:dev": "export NODE_ENV=development && webpack --config build/webpack.dev.js",
        "build:prod": "export NODE_ENV=production && webpack --config build/webpack.prod.js",
        "package": "bash scripts/package.bash"
    },
    "dependencies": {
        "@babel/core": "^7.26.10",
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/preset-env": "^7.20.2",
