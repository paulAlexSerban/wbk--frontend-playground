        "babel-loader": "^9.1.2",
        "clean-webpack-plugin": "^4.0.0",
        "copy-webpack-plugin": "^13.0.0",
        "css-loader": "^6.7.3",
        "dotenv": "^16.3.1",
        "eslint-plugin-import": "^2.27.5",
        "eslint-webpack-plugin": "^4.0.0",
        "events": "^3.3.0",
        "handlebars": "^4.7.8",
        "handlebars-loader": "^1.7.3",
        "html-entities": "^2.3.3",
        "html-webpack-plugin": "^5.5.0",
        "marked": "^7.0.4",
        "mini-css-extract-plugin": "^2.7.2",
        "sass": "^1.86.3",
        "sass-loader": "^13.2.0",
        "sass-migrator": "^2.3.1",
        "scrollreveal": "^4.0.9",
