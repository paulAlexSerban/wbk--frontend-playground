    "scripts": {
        "getAssets": "rm -rfv ./dist/assets && mkdir ./dist/assets && cp -rfv ../../assets/dist/* ./dist/assets",
        "watch": "npm run getAssets && export NODE_ENV=development && webpack serve --config webpack.dev.js --hot --progress",
