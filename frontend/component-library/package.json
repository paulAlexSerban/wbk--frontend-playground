        "eslint-webpack-plugin": "^4.0.0",
        "handlebars-loader": "^1.7.3",
        "html-webpack-plugin": "^5.5.0",
        "mini-css-extract-plugin": "^2.7.2",
        "sass": "^1.58.1",
        "sass-loader": "^13.2.0",
        "source-map-loader": "^4.0.1",
        "style-loader": "^3.3.2",
